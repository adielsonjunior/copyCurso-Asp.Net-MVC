@model List<MaromFit.Models.Plan>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@if (Model.Count > 0)
{
<h1>Planos</h1>
<ul>
    @foreach (var plano in Model)
    {
        <li>@plano.Name / @plano.Value R$</li>
    }
</ul>
}
else
{
    <p>Não Existem planos cadastrados no sistema!</p>
}